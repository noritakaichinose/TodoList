{
  "version": 2,
  "builds": [
    {
      "src": "front/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "back/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/back/api/$1" },
    { "src": "/(.*)", "dest": "/front/$1" }
  ]
}